{
  "schemaVersion": 1,
  "id": "griefingutils",
  "version": "${version}",
  "name": "0x06's Griefing Utils",
  "description": "An addon that makes griefing faster and easier than ever before.",
  "authors": ["0x06", "DuckGuy"],
  "contact": {
    "homepage": "https://github.com/0x000006/griefing-utils",
    "issues": "https://github.com/0x000006/griefing-utils/issues",
    "repo": "https://github.com/0x000006/griefing-utils",
    "sources": "https://github.com/0x000006/griefing-utils"
  },
  "license": "MIT-0",
  "icon": "assets/griefing_utils/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "griefingutils.GriefingUtils"
    ]
  },
  "mixins": ["griefingutils.mixins.json"],
  "custom": {
    "repo": {
      "branch": "${repo_branch}",
      "commit": "${commit}",
      "id": "${repo_id}"
    },
    "meteor-client:color": "255,75,0",
    "modmenu": {
      "links": {
        "modmenu.github_releases": "https://github.com/0x000006/griefing-utils/releases"
      }
    }
  },
  "accessWidener": "griefingutils.accesswidener",
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
